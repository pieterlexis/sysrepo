#!/usr/bin/make -f
# -*- makefile -*-
# Uncomment this to turn on verbose mode.
export DH_VERBOSE=1

%:
	dh $@

override_dh_strip:
	dh_strip -p@PACKAGE@@SYSREPO_SOVERSION@  --dbg-package=@PACKAGE@@SYSREPO_SOVERSION@-dbg
	dh_strip -p@CPP_PACKAGE@@SYSREPO_SOVERSION@ --dbg-package=@CPP_PACKAGE@@SYSREPO_SOVERSION@-dbg
#	dh_strip -p@PYTHON_PACKAGE@ --dbg-package=@PYTHON_PACKAGE@-dbg

override_dh_auto_configure:
	cmake -DCMAKE_INSTALL_PREFIX=/usr -DCMAKE_BUILD_TYPE="Package" -DGEN_LANGUAGE_BINDINGS=ON -DGEN_PYTHON_BINDINGS=OFF .

override_dh_auto_test:
	SYSREPO_REPOSITORY_PATH=$PWD ctest --output-on-failure
